<Application
    x:Class="MagickCrop.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MagickCrop"
    xmlns:converters="clr-namespace:MagickCrop.Converters"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
            </ResourceDictionary.MergedDictionaries>
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <converters:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter"/>
            <converters:ColorToBrushConverter x:Key="ColorToBrushConverter"/>
            <converters:SubtractHalfConverter x:Key="SubtractHalfConverter"/>
            <converters:AngleArcPathConverter x:Key="AngleArcPathConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
